{
    "manifest_version": 3,
    "name": "Page Tracker",
    "version": "1.0",
    "description": "Tracks and searches your previously opened web pages, including incognito mode on demand.",
    "permissions": [
      "tabs",
      "history",
      "storage",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "options.html",
    "incognito": "split"
  }
  